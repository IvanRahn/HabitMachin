 <%=hash["date"]%>
 
 <div class="square sm">
   <div class="square-row">
    <% for id in 1..6 %>
     <div class="symbol">
      <label for="" class="checkbox-label-checked" ><%=hash["h#{id}"] if hash.has_key?("h#{id}") && hash["h#{id}"] != "0"%></label></div>
    <%end%>
 </div>
  <div class="square-row">
    <% for id in 7..12 %>
   <div class="symbol">
      <label for="" class="checkbox-label-checked" ><%=hash["h#{id}"] if hash.has_key?("h#{id}") && hash["h#{id}"] != "0"%></label></div>
    <%end%>
 </div>
 </div>
 <% if !hash["waking_time"].nil? and !hash["sleep_time"].nil?%>
   <div class="time">
   <%=hash["waking_time"].strftime("%H:%M")%>
   <%=hash["sleep_time"].strftime("%H:%M")%>
  </div> 
  <%end%>
 </div>  
  